{
  "swagger": "2.0",
  "info": {
    "description": "전기자동차 충전소 안내를 위한 REST API 서버 프로젝트",
    "version": "1.0.0",
    "title": "전기자동차 충전소 알림이 - Green Plug",
    "termsOfService": "https://docs.google.com/document/d/1dWoRca-LThHFuFSTpoI2KnGW_HQgYdErCXr8BsdccEE/edit?usp=sharing",
    "contact": {
      "name": "Juyong Lee",
      "email": "apiteam@swagger.io"
    },
    "license": {
      "name": "MIT License",
      "url": "https://github.com/juyonglee/EV-Sataion-ToyProject/blob/master/LICENSE"
    }
  },
  "host": "localhost:3000",
  "basePath": "",
  "tags": [
    {
      "name": "user",
      "description": "Operations about user",
      "externalDocs": {
        "description": "Find out more about our store",
        "url": "http://swagger.io"
      }
    }
  ],
  "schemes": [
    "http"
  ],
  "paths": {
    "/testset": {
      "get": {
        "tags": [
          "Manager"
        ],
        "summary": "Test Set의 상태를 나타낸다.",
        "description": "Test Set의 총 데이터의 개수를 나타낸다.",
        "produces": [
          "text/html",
          "application/xml",
          "application/json"
        ],
        "responses": {
          "default": {
            "description": "successful operation"
          }
        }
      }
    }
  },
  "securityDefinitions": {
    "petstore_auth": {
      "type": "oauth2",
      "authorizationUrl": "http://petstore.swagger.io/oauth/dialog",
      "flow": "implicit",
      "scopes": {
        "write:pets": "modify pets in your account",
        "read:pets": "read your pets"
      }
    },
    "api_key": {
      "type": "apiKey",
      "name": "api_key",
      "in": "header"
    }
  },
  "definitions": {
    "Station": {
      "type": "object",
      "properties": {
        "sid": {
          "type": "integer",
          "format": "int64",
          "description": "충전소 ID"
        },
        "snm": {
          "type": "string",
          "description": "충전소 이름"
        },
        "cid": {
          "type": "string",
          "description": "충전기 댓수"
        },
        "ctp": {
          "type": "string",
          "description": "1: DC차데모\n2: AC완속\n3: DC차데모, AC3상\n4: DC콤보\n5: DC차데모, DC콤보\n6: DC차데모, AC3상, DC콤보"
        },
        "cst": {
          "type": "string",
          "description": "1: 통신미연결, 2: 충전대기, 3: 충전중, 4: 운영중지, 5: 점검중, 7: 시범운영"
        },
        "hol": {
          "type": "string",
          "description": "휴무여부"
        },
        "p": {
          "type": "integer",
          "format": "int64",
          "description": "주차비무료여부 (0:무료, 1:유료)"
        },
        "ut": {
          "type": "string",
          "description": "이용시간정보"
        },
        "x": {
          "type": "number",
          "format": "double"
        },
        "y": {
          "type": "number",
          "format": "double"
        }
      },
      "xml": {
        "name": "User"
      }
    }
  }
}